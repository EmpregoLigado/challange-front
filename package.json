{
  "name": "challange-front",
  "version": "0.2.0",
  "description": "Simple test project to consume a API, show the holiday dates and more",
  "homepage": "https://nelsera.github.io/challange-front",
  "repository": {
    "type": "git",
    "url": "git://github.com/nelsera/challange-front.git"
  },
  "devDependencies": {
    "babel-jest": "^16.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "enzyme": "^2.5.1",
    "gh-pages": "^0.12.0",
    "jest": "^16.0.2",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-addons-test-utils": "^15.3.2",
    "react-scripts": "0.7.0",
    "react-test-renderer": "^15.3.2",
    "standard-version": "^4.0.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.3.2",
    "react-infinite-calendar": "^2.1.5",
    "whatwg-fetch": "^1.0.0"
  },
  "scripts": {
    "preinstall": "npm install -g yarn",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "release": "standard-version"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "verbose": true
  },
  "license": "MIT",
  "engines": {
    "node": "= 7.6",
    "npm": ">= 3.10"
  }
}
